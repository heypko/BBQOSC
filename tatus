[32m+[m[32m// launch as server for BBQ_client_w10.ckbbq[m
[32m+[m
[32m+[m[32m// BBQ by Hana Shin[m
[32m+[m[32m// 2014[m
[32m+[m
[32m+[m[32m// b,,b,,q, b,,b,,q, b,,b,,q, b,,b,,q,[m[41m [m
[32m+[m[32m// b,spamq b,spamq b,spamq b,spamg bv,szzw[m
[32m+[m[32m// bdot,ms bdot,ms bdot,ms bdoamar[m
[32m+[m[32m// bdty,,a, bdtyhha, bdt,bdt, ffszvcp,[m
[32m+[m[32m// bdtfsi,, bdtfsiu, b,ssvj,, dahda,a,[m
[32m+[m[32m// blop,op, blop,op, blop,op, bloblop,[m[41m [m
[32m+[m[32m// sndbufArray[sbc],,, sponcc,, bswqjspf bahsz,,,[m
[32m+[m
[32m+[m[32m// rahrah,,roar,rr,r,grr,roar,ra[m
[32m+[m[32m// rahrah,,roar,rr,r,grr,roar,rah[m
[32m+[m[32m// lollpop lollipoplollpop lollpom[m
[32m+[m[32m// lollpop lollipoploll[m
[32m+[m[32m// munch,y, munchii, munomnom umnumnua[m[41m [m
[32m+[m
[32m+[m[32m// wumpow,, wum,now, wumpowp lollpur[m
[32m+[m[32m// s,snap,, oshnaps, sndbufArray[sbc]sloc, snapz[m
[32m+[m[32m// zz,sfaz, zz,asazz ,asfzza, shizzle[m
[32m+[m
[32m+[m[32m// b,,b,,q, b,,b,,q, b,,b,,q, b,,b,,q,[m[41m [m
[32m+[m[32m// b,,b qii, b,,b qui b,uibuy tstsha,,[m[41m [m
[32m+[m[32m// b,sizzle b,sizzle b,sizzle shizip,, a[m
[32m+[m
[32m+[m[32m0 => int startCHAN;                 // Audio Channel Start Index[m
[32m+[m[32m1 => int endCHAN;                   // Audio Channel End Index[m
[32m+[m
[32m+[m[32m1.0 => float numpadRate;[m
[32m+[m
[32m+[m[32m0 => int sbc;                       // sndbufArray Counter[m
[32m+[m[32m100 => int instanceCount;           // size of sndbufArray[m
[32m+[m[32mSndBuf sndbufArray[instanceCount];  // Initialize sndbufArray[m
[32m+[m
[32m+[m[32mfun void playSighReverse() // a[m
[32m+[m[32m{[m
[32m+[m[32m    10 => int startPos;[m
[32m+[m[32m    250 => int len;[m
[32m+[m[32m    <<<sbc>>>;[m
[32m+[m
[32m+[m[32m    // a[m
[32m+[m[32m    sndbufArray[sbc] => ADSR e_sighReverse => Pan2 p_sighReverse => dac.chan(Std.rand2(startCHAN,endCHAN));[m
[32m+[m[32m    me.sourceDir() + "/khaa.wav" => string fname;[m
[32m+[m[32m    fname => sndbufArray[sbc].read;[m
[32m+[m[32m    0.7 => sndbufArray[sbc].gain;[m
[32m+[m[32m    1 => sndbufArray[sbc].rate;[m
[32m+[m[32m    Std.rand2f( -0.8, 0.8 ) => p_sighReverse.pan;[m
[32m+[m[32m    e_sighReverse.set( 10::ms, 10::ms, 0.9, 100::ms );[m
[32m+[m[41m    [m
[32m+[m[32m    (startPos*44.100) $ int => sndbufArray[sbc].pos;[m
[32m+[m[32m    e_sighReverse.keyOn();[m
[32m+[m[32m    len::ms => now;[m
[32m+[m[32m    e_sighReverse.keyOff();[m
[32m+[m[32m    10::ms => now;[m[41m   [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfun void playBongo() // b[m
[32m+[m[32m{[m
[32m+[m[32m    0 => int startPos;[m
[32m+[m[32m    1000 => int len;[m
[32m+[m[32m    <<<sbc>>>;[m
[32m+[m[32m    // bongo[m[41m [m
[32m+[m[32m    sndbufArray[sbc] => ADSR e_bongo => Pan2 p_bongo => dac.chan(startCHAN);[m
[32m+[m[32m    me.sourceDir() + "/bongo.wav" => sndbufArray[sbc].read;[m
[32m+[m[32m    .5 => sndbufArray[sbc].gain;[m
[32m+[m[32m    1.1 => sndbufArray[sbc].rate;[m
[32m+[m[32m    Std.rand2f( -0.8, 0 ) => p_bongo.pan;[m
[32m+[m[32m    e_bongo.set( 10::ms, 10::ms, 0.6, 100::ms );[m
[32m+[m[41m    [m
[32m+[m[32m    (startPos*44.100) $ int => sndbufArray[sbc].pos;[m
[32m+[m[32m    e_bongo.keyOn();[m
[32m+[m[32m    len::ms => now;[m
[32m+[m[32m    e_bongo.keyOff();[m
[32m+[m[32m    10::ms => now;[m
[32m+[m[41m    [m
[32m+[m[32m    sndbufArray[sbc] =< e_bongo;[m
[32m+[m[32m    e_bongo =< p_bongo;[m
[32m+[m[32m    p_bongo =< dac;[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfun void playChoke() // c[m
