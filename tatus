[32m+[m
[32m+[m[32mfun void play( SndBuf sample, int startPos, ADSR env, int len ) // in ms[m
[32m+[m[32m{[m
[32m+[m[32m    (startPos*44.100) $ int => sample.pos;[m
[32m+[m[32m    env.keyOn();[m
[32m+[m[32m    len::ms => now;[m
[32m+[m[32m    env.keyOff();[m
[32m+[m[32m    10::ms => now;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// create our OSC receiver[m
[32m+[m[32mOscIn oin;[m
[32m+[m[32m// create our OSC message[m
[32m+[m[32mOscMsg msg;[m
[32m+[m[32m// use port 6449[m
[32m+[m[32m6449 => oin.port;[m
[32m+[m[32m// create an address in the receiver[m
[32m+[m[32moin.addAddress( "/sndbuf/bbq/number, i" );[m
[32m+[m
[32m+[m
[32m+[m[32m// infinite event loop[m
[32m+[m[32mwhile( true )[m
[32m+[m[32m{[m[41m       [m
[32m+[m[32m    // wait for event to arrive[m
[32m+[m[32m    oin => now;[m
[32m+[m
[32m+[m[32m    // grab the next message from the queue.[m[41m [m
[32m+[m[32m    while ( oin.recv(msg) != 0 )[m
[32m+[m[32m    {[m[41m   [m
[32m+[m[32m        // getFloat fetches the expected float (as indicated by "f")[m
[32m+[m[32m        msg.getInt(0) => int which;[m
[32m+[m[32m        // print[m
[32m+[m[32m        <<< "parsing (via OSC):", which >>>;[m
[32m+[m[32m        // pass to player[m
[32m+[m[32m        parseKeys( which );[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// function that parses keyboard presses[m[41m [m
[32m+[m[32mfun void parseKeys( int which )[m
[32m+[m[32m{[m
[32m+[m[32m    // match numbers to letters and increment the appropriate layer variable[m[41m [m
[32m+[m[32m    if( which == 30 ) { // a[m
[32m+[m[32m        spork ~playSighReverse();[m
[32m+[m[32m        (sbc+1)%instanceCount => sbc;[m
[32m+[m[32m    }[m
[32m+[m[32m    else if( which == 48 ) { // b[m
[32m+[m[32m        spork ~playBongo();[m
[32m+[m[32m        (sbc+1)%instanceCount => sbc;[m
[32m+[m[32m    }[m[41m   [m
[32m+[m[32m    else if( which == 46 ) { // c[m
[32m+[m[32m        spork ~playChoke();[m
[32m+[m[32m        (sbc+1)%instanceCount => sbc;[m
[32m+[m[32m    }[m
