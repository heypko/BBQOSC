[32m+[m[32mfun void playSteam() // f[m
[32m+[m[32m{[m
[32m+[m[32m    500 => int startPos;[m
[32m+[m[32m    200 => int len;[m
[32m+[m[41m    [m
[32m+[m[32m    // f[m
[32m+[m[32m    sndbufArray[sbc] => ADSR e_steam => Pan2 p_steam => dac.chan(Std.rand2(startCHAN,endCHAN));[m
[32m+[m[32m    me.sourceDir() + "/steam.wav" => sndbufArray[sbc].read;[m
[32m+[m[32m    9 => sndbufArray[sbc].gain;[m
[32m+[m[32m    1.0 => sndbufArray[sbc].rate;[m
[32m+[m[32m    Std.rand2f( -0.8, 0.8 ) => p_steam.pan;[m
[32m+[m[32m    e_steam.set( 100::ms, 1::ms, 0.9, 50::ms );[m
[32m+[m[41m    [m
[32m+[m[32m    (startPos*44.100) $ int => sndbufArray[sbc].pos;[m
