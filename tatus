[32m+[m[32m// launch as performer for BBQ_stereOSC[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m    This IP needs to be changed[m
[32m+[m[32m    to the address of the server[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m// host name and port[m
[32m+[m[32m"localhost" => string hostname;[m
[32m+[m[32m6449 => int port;[m
[32m+[m[32m"notmac" => string choice;[m
[32m+[m[32m1 => int pan;[m
[32m+[m[32m0 => int deviceNum;[m
[32m+[m
[32m+[m[32m// get command line[m
[32m+[m[32mif( me.args() ) me.arg(0) => hostname;[m
[32m+[m[32mif( me.args() > 1 ) me.arg(1) => choice;[m
[32m+[m[32mif( me.args() > 2 ) me.arg(2) => Std.atoi => pan;[m
[32m+[m[32mif( me.args() > 3 ) me.arg(3) => Std.atoi => deviceNum;[m
[32m+[m[41m    [m
[32m+[m
[32m+[m[32m// send object[m
[32m+[m[32mOscSend xmit;[m
[32m+[m
[32m+[m[32m// aim the transmitter[m
[32m+[m[32mxmit.setHost( hostname, port );[m
[32m+[m
[32m+[m[32m// instantiate a HidIn object[m
[32m+[m[32mHid hi;[m
[32m+[m[32m//KBHit kb;[m
[32m+[m
[32m+[m[32m// structure to hold HID messages[m
[32m+[m[32mHidMsg msg;[m
[32m+[m
[32m+[m[32m// open keyboard[m
[32m+[m[32mif( !hi.openKeyboard( deviceNum ) ) me.exit();[m
[32m+[m[32m// successful! print name of device[m
[32m+[m[32m<<< "keyboard '", hi.name(), "' ready" >>>;[m
[32m+[m
[32m+[m[32m//if ( choice == "mac" ) {[m[41m                [m
[32m+[m[32m//    parseMac(msg.which) => xmit.addInt;[m
[32m+[m[32m//} else {[m
[32m+[m[32m//    msg.which => xmit.addInt;[m
[32m+[m[32m//}[m
[32m+[m
[32m+[m[32m// infinite time loop[m
[32m+[m[32mwhile( true )[m
[32m+[m[32m{[m[41m      [m
[32m+[m[32m    hi => now;[m
[32m+[m[41m    [m
[32m+[m[32m    //get one or more messages[m
[32m+[m[32m    while( hi.recv( msg ) )[m
[32m+[m[32m    {[m
[32m+[m[32m        // check for action type[m
[32m+[m[32m        if( msg.isButtonDown() )[m
[32m+[m[32m        {[m
[32m+[m[32m            // start the message...[m
[32m+[m[32m            xmit.startMsg( "/sndbuf/bbq/number", "i f" );[m
[32m+[m[41m            [m
[32m+[m[32m            int num;[m
[32m+[m[32m            if ( choice == "mac" ) {[m[41m                [m
[32m+[m[32m                parseMac(msg.which) => xmit.addInt;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                msg.which => xmit.addInt;[m
[32m+[m[32m            }[m
[32m+[m[32m            pan => xmit.addFloat;[m
[32m+[m[41m            [m
[32m+[m[32m            // send[m
[32m+[m[32m            <<< "sent (via OSC):", num, pan >>>;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfun int parseMac( int which )[m
[32m+[m[32m{[m
[32m+[m[32m    int num;[m
[32m+[m[32m    if( which == 4 ) // a[m
[32m+[m		[32m30 => num;[m
[32m+[m[32m    else if( which == 5 ) // b[m
[32m+[m		[32m48 => num;[m
[32m+[m[32m    else if( which == 6 ) // c[m
[32m+[m		[32m46 => num;[m
[32m+[m[32m    else if( which == 7 ) // d[m
[32m+[m		[32m32 => num;[m
[32m+[m[32m    else if( which == 8 ) // e[m
[32m+[m		[32m18 => num;[m
[32m+[m[32m    else if( which == 9 ) // f[m
[32m+[m		[32m33 => num;[m
[32m+[m[32m    else if( which == 10 ) // g[m
[32m+[m		[32m34 => num;[m
[32m+[m[32m    else if( which == 11 ) // h[m
[32m+[m		[32m35 => num;[m
[32m+[m[32m    else if( which == 12 ) // i[m
[32m+[m		[32m23 => num;[m
[32m+[m[32m    else if( which == 13 ) // j[m
[32m+[m		[32m36 => num;[m
[32m+[m[32m    else if( which == 15 ) // l[m
[32m+[m		[32m38 => num;[m
[32m+[m[32m    else if( which == 16 ) // m[m
[32m+[m		[32m50 => num;[m
[32m+[m[32m    else if( which == 17 ) // n[m
[32m+[m		[32m49 => num;[m
[32m+[m[32m    else if( which == 18 ) // o[m
[32m+[m		[32m24 => num;[m
[32m+[m[32m    else if( which == 19 ) // p[m
[32m+[m		[32m25 => num;[m
[32m+[m[32m    else if( which == 20 ) // q[m
[32m+[m		[32m16 => num;[m
[32m+[m[32m    else if( which == 21 ) // r[m
[32m+[m		[32m19 => num;[m
[32m+[m[32m    else if( which == 22 ) // s[m
[32m+[m		[32m31 => num;[m
[32m+[m[32m    else if( which == 23 ) // t[m
[32m+[m		[32m20 => num;[m
[32m+[m[32m    else if( which == 24 ) // u[m
[32m+[m		[32m22 => num;[m
[32m+[m[32m    else if( which == 25 ) // v[m
[32m+[m		[32m47 => num;[m
[32m+[m[32m    else if( which == 26 ) // w[m
[32m+[m		[32m17 => num;[m
[32m+[m[32m    else if( which == 28 ) // y[m
[32m+[m		[32m21 => num;[m
[32m+[m[32m    else if( which == 29 ) // z[m
[32m+[m[32m        44 => num;[m
[32m+[m[32m}[m
